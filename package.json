{
  "name": "@wunderbon/json-schemas",
  "description": "wunderbon **JSON-Schemas** as blueprints for data structures and contracts like interfaces, models and helpers. Always up to date with the latest API specification.",
  "homepage": "https://wunderbon.app",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/wunderbon/json-schemas.git"
  },
  "bugs": {
    "url": "https://bitbucket.org/wunderbon/json-schemas/issues"
  },
  "version": "v0.3.2",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "main": "./server.js",
  "scripts": {
    "build": "grunt build",
    "test": "echo \"Error: n.a.\" && exit 1",
    "start": "set NODE_ENV=production && node ./server.js",
    "development": "set NODE_ENV=development && nodemon -L ./server.js"
  },
  "directories": {
    "doc": "docs"
  },
  "keywords": [
    "json",
    "schema",
    "wunderbon",
    "model"
  ],
  "author": "wunderbon International B.V.",
  "maintainers": [
    {
      "name": "wunderbon International B.V.",
      "email": "open-source@wunderbon.io",
      "web": "https://developers.wunderbon.io"
    }
  ],
  "nyc": {
    "watermarks": {
      "lines": [
        75,
        75
      ],
      "functions": [
        75,
        75
      ],
      "branches": [
        75,
        75
      ],
      "statements": [
        75,
        75
      ]
    },
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "bin/*",
      "**/*interface.js",
      "**/*.ts",
      "**/*.d.ts",
      "**/*-test.js",
      "test/helper.js",
      "fixtures/**/*",
      "test/**/*",
      "demo/**/*",
      "docs/**/*",
      "app/model/*",
      "app/type/*",
      "stryker.conf.js",
      "./*.js"
    ],
    "report-dir": "./docs/report/coverage",
    "all": true,
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/chai-as-promised": "^7.1.2",
    "@types/chai-http": "^4.2.0",
    "@types/eslint": "^6.1.8",
    "@types/grunt": "^0.4.25",
    "@types/mocha": "^7.0.1",
    "@types/nock": "^11.1.0",
    "ajv": "^6.12.0",
    "ajv-cli": "^3.1.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-async-to-promises": "^1.0.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "codacy-coverage": "^3.4.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "dts-bundle-generator": "^3.3.1",
    "dts-generator": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "express": "^4.17.1",
    "grunt": "^1.0.4",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-eslint": "^22.0.0",
    "grunt-move": "^1.0.3",
    "grunt-prepend": "^0.1.2",
    "grunt-run": "^0.8.1",
    "grunt-ts": "^6.0.0-beta.22",
    "grunt-tslint": "^5.0.2",
    "jquery": "^3.4.1",
    "json-dereference-cli": "^0.1.2",
    "json-schema-faker": "^0.5.0-rc23",
    "json-schema-faker-cli": "^2.0.8",
    "json-schema-ref-parser": "^7.1.3",
    "json-schema-to-typescript": "^8.1.0",
    "mocha": "^7.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-typescript": "^1.1.17",
    "nock": "^12.0.1",
    "node-docson": "^0.4.6",
    "nodemon": "^2.0.2",
    "npm-dts": "^1.2.0",
    "nyc": "^15.0.0",
    "quicktype": "^15.0.214",
    "serve-static": "^1.14.1",
    "showdown": "^1.9.1",
    "source-map-support": "^0.5.16",
    "stryker": "^0.35.1",
    "stryker-api": "^0.24.1",
    "stryker-html-reporter": "^0.18.1",
    "stryker-javascript-mutator": "^0.14.1",
    "stryker-mocha-framework": "^0.15.1",
    "stryker-mocha-runner": "^0.17.1",
    "swagger-ui-express": "^4.1.3",
    "ts-mockito": "^2.5.0",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.8.3"
  }
}
