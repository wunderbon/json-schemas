{
  "$id": "https://bitbucket.org/wunderbon/json-schemas/raw/v0.4.15/schema/gps/gps.schema.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "version": "v0.4.15",
  "contact": {
    "name": "wunderbon Operation GmbH & Co. KG",
    "url": "https://developers.wunderbon.io",
    "email": "open-source@wunderbon.io"
  },
  "title": "GPS",
  "description": "A GPS coordinates representation within the wunderbon domain.",
  "type": "object",
  "properties": {
    "latitude": {
      "description": "Latitude of the GPS coordinate.",
      "type": "number",
      "examples": [
        37.426473
      ]
    },
    "longitude": {
      "description": "Longitude of the GPS coordinate.",
      "type": "number",
      "examples": [
        -122.193146
      ]
    }
  },
  "required": [
    "latitude",
    "longitude"
  ],
  "additionalProperties": false
}


