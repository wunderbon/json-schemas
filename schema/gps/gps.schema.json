{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://bitbucket.org/wunderbon/json-schemas/raw/master/schema/gps/gps.schema.json",
  "title": "GPS",
  "description": "A GPS coordinates representation in wunderbon domain.",
  "definitions": {
    "gps": {
      "type": "object",
      "properties": {
        "latitude": {
          "type": "number"
        },
        "longitude": {
          "type": "number"
        }
      },
      "additionalProperties": false,
      "required": [
        "latitude",
        "longitude"
      ]
    }
  }
}


