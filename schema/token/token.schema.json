{
  "$id": "https://bitbucket.org/wunderbon/json-schemas/raw/master/schema/token/token.schema.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "version": "v0.4.19",
  "contact": {
    "name": "wunderbon Operation GmbH & Co. KG",
    "url": "https://developers.wunderbon.io",
    "email": "open-source@wunderbon.io"
  },
  "title": "Token",
  "description": "Token representation within the wunderbon domain.",
  "type": "object",
  "properties": {
    "value": {
      "description": "Street and Number as well as token additions.",
      "type": "string",
      "minLength": 6,
      "maxLength": 512,
      "examples": [
        "kMgncWoSSGY0je/b4WhW3WupFo4N83skfvT4kDm2uZigSlSzCfh0NT3tgRR2n5VAEhxxfJxQEhv/LA9VmwOirw6lzwYR7Ori4lr2+dj4Ox3L2aj8VytF13OvuY7dTonQ"
      ]
    },
    "type": {
      "description": "Type of the token.",
      "$ref": "https://bitbucket.org/wunderbon/json-schemas/raw/master/schema/token/tokenType.schema.json"
    }
  },
  "additionalProperties": false,
  "required": [
    "value",
    "type"
  ]
}
