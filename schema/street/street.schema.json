{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://bitbucket.org/wunderbon/json-schemas/raw/master/schema/street/street.schema.json",
  "title": "Street",
  "description": "A street representation in wunderbon domain.",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 3,
      "maxLength": 255
    },
    "number": {
      "type": "integer"
    },
    "additional": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    }
  },
  "additionalProperties": false,
  "required": [
    "name",
    "number"
  ]
}


