{
  "$id": "https://bitbucket.org/wunderbon/json-schemas/raw/v0.4.15/schema/cashier/cashier.schema.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "version": "v0.4.15",
  "contact": {
    "name": "wunderbon Operation GmbH & Co. KG",
    "url": "https://developers.wunderbon.io",
    "email": "open-source@wunderbon.io"
  },
  "title": "Cashier",
  "description": "An cashier representation within the wunderbon domain.",
  "type": "object",
  "properties": {
    "name": {
      "description": "Name of the cashier.",
      "oneOf": [
        {
          "$ref": "https://bitbucket.org/wunderbon/json-schemas/raw/v0.4.15/schema/name/peopleName.schema.json"
        },
        {
          "description": "Some Id for example.",
          "type": "string",
          "maxLength": 255,
          "examples": [
            "123",
            "John Doe",
            "Jane"
          ]
        }
      ]
    }
  },
  "required": [
    "name"
  ],
  "additionalProperties": false
}
